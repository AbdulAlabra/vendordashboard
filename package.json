{
  "name": "react-material-dashboard",
  "author": "DeviasIO",
  "email": "contact@devias.io",
  "licence": "MIT",
  "version": "0.3.0",
  "private": false,
  "scripts": {
    "start": "env-cmd -e development react-scripts start",
    "start-prod": "env-cmd -e production react-scripts start",
    "start-staging": "env-cmd -e staging react-scripts start",
    "build-dev": "env-cmd -e development react-scripts build",
    "build-staging": "env-cmd -e staging react-scripts build",
    "build-prod": "env-cmd -e production GENERATE_SOURCEMAP=false react-scripts build",
    "deploy-dev": "firebase use staging && firebase deploy",
    "deploy-prod": "firebase use production && firebase deploy",
    "test": "env-cmd -e test react-scripts test",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": "12.x.x"
  },
  "type": "module",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.1",
    "axios": "^0.20.0",
    "bson-objectid": "^1.3.1",
    "capitalize": "^2.0.3",
    "chart.js": "^2.8.0",
    "clsx": "^1.0.4",
    "dayjs": "^1.9.6",
    "detectlanguage": "^2.0.0",
    "env-cmd": "^10.1.0",
    "history": "^4.9.0",
    "i18next": "^19.8.2",
    "i18next-browser-languagedetector": "^6.0.1",
    "joi": "^17.2.1",
    "jss-rtl": "^0.3.0",
    "language-identifier": "^1.0.8",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.6",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-google-maps": "^9.4.5",
    "react-i18next": "^11.7.3",
    "react-perfect-scrollbar": "^1.5.3",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.4.4",
    "react-transition-group": "^4.4.1",
    "realm-web": "^1.0.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "typescript": "^3.5.1"
  }
}
